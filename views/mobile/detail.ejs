<% include ./header %>
<%- include(detail) %>
<script type="text/javascript">
	$(function() {
		$('.navbar ul li a').on('click', function (e) {
        		var uri = $(e.target).data('href');
        		if(uri.substr(uri.length-1) != '#') {
        			location.href = uri + '?act=other';	
        		}
        		else {
        			location.href = uri;
        		}
            $('.navbar').addClass('hide');
        });
	})
</script>
